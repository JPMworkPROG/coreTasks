NODE_ENV=development
SERVER_PORT=3000
SERVER_RATE_LIMIT_TTL_SECONDS=10
SERVER_RATE_LIMIT_LIMIT=100

# CORS Configuration
# Múltiplas origens separadas por vírgula
# Use '*' para permitir todas (não recomendado em produção)
CORS_ORIGIN=http://localhost:8080,http://localhost:3001,http://localhost:8081

# RabbitMQ Configuration
RABBITMQ_URL=amqp://admin:admin@localhost:5672
RABBITMQ_QUEUE_DURABLE=false
RABBITMQ_REQUEST_TIMEOUT_MS=5000
RABBITMQ_AUTH_QUEUE=auth.queue
RABBITMQ_TASK_QUEUE=tasks.queue
RABBITMQ_USER_QUEUE=users.queue

# JWT Configuration
JWT_ACCESS_SECRET=your-super-secret-access-key-change-in-production
JWT_REFRESH_SECRET=your-super-secret-refresh-key-change-in-production
JWT_ACCESS_EXPIRES_IN=15m
JWT_REFRESH_EXPIRES_IN=7d
